{
  "name": "@tsed/root",
  "version": "3.0.1",
  "description": "Node.js wrapper of C library libgphoto2 written in TypeScript",
  "author": "Romain Lenzotti",
  "private": true,
  "contributors": [
    "Alexander Gusez (https://github.com/goooseman)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypedProject/ts-gphoto2-driver.git"
  },
  "bugs": {
    "url": "https://github.com/TypedProject/ts-gphoto2-driver/issues"
  },
  "homepage": "https://github.com/TypedProject/ts-gphoto2-driver",
  "scripts": {
    "configure": "monorepo ci configure",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint packages --ext .ts",
    "test:lint:fix": "eslint packages --ext .ts --fix",
    "build": "monorepo build",
    "prettier": "prettier '{packages,test}/**/*.ts' --write",
    "release": "semantic-release",
    "release:dryRun": "semantic-release --dry-run"
  },
  "dependencies": {
    "lerna": "4.0.0",
    "@tsed/monorepo-utils": "1.17.0",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "cross-env": "7.0.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-prettier": "3.1.4",
    "gflow": "5.1.2",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.4.0",
    "prettier": "2.2.1",
    "segfault-handler": "1.3.0",
    "semantic-release": "17.4.2",
    "ts-node": "9.1.1",
    "ts-jest": "26.5.5",
    "typescript": "4.2.4"
  },
  "devDependencies": {},
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "monorepo": {
    "productionBranch": "production",
    "developBranch": "production",
    "npmAccess": "public"
  }
}
